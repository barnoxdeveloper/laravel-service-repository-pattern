{
    "name": "barnoxdev/laravel-service-repository-pattern",
    "description": "A Laravel package to create service and repository patterns easily",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "BarnoxDev\\ServiceRepositoryPattern\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BarnoxDev\\ServiceRepositoryPattern\\LaravelServiceRepositoryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
